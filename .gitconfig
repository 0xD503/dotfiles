[alias]
	st = status
	co = checkout
	ci = commit
	br = branch
	unload = push --all
	stu = status -uno
	df = diff
	lg = log --oneline --graph
	mg = merge
	rb = rebase
	dfc = diff --cached
	adu = add -u
	cfd = clean -fd
	rs = restore
	rss = restore --staged
	adi = add -i
	cim = commit -m
	cl = clone
	ciam = commit -am
	rmc = rm --cached
	sw = switch
[user]
	name = Ruslan Vostretsov
	email = ElectricalBrain@protonmail.com
[core]
	editor = emacs -nw
[init]
	defaultBranch = master
